doctype 5
html
  head
    title= title
    script(src='http://www.parsecdn.com/js/parse-1.3.4.min.js')
    script(src='/js/parseAPI.js')
    script(src='/js/index.js')
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    #header
      #title
        h1
          a(href="/") ShareIt!

      #navigation
        #search
          input#searchbox
          button#searchbutton Search
        #buttons
          a(href="/") Home
          a(href="/list") List
          a(href="/profile?id="+userID) Profile
          a(href="#")#logout Logout

    block content
